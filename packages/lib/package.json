{
  "name": "@orbs-network/twap-ui",
  "version": "0.0.1",
  "description": "TWAP UI",
  "license": "MIT",
  "author": "Orbs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbs-network/twap-ui.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "prettier": "prettier -w '{src,test}/**/*.{ts,tsx,js,jsx,json,sol}'",
    "build": "npm run prettier && rm -rf dist && tsc",
    "start": "npm run prettier && nodemon --ext js,jsx,ts,tsx,json --watch ./src --exec tsc",
    "test": "tsc && for t in eth ftm; do echo \"testing $t...\"; NETWORK=$t DOTENV_CONFIG_PATH=../../.env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r dotenv/config -r ts-node/register 'test/**/*.test.ts'; done"
  },
  "dependencies": {
    "@defi.org/web3-candies": "4.x",
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@mui/material": "5.x",
    "ethereum-multicall": "2.x",
    "moment": "2.x",
    "react-icons": "4.x",
    "react-number-format": "5.x",
    "react-query": "3.x",
    "zustand": "4.x",
    "@open-defi-notification-protocol/widget": "0.0.x"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "config": {
    "omit": "optional"
  }
}
