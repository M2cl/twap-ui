{
  "name": "@orbs-network/twap-ui",
  "version": "0.0.1",
  "description": "TWAP UI",
  "license": "MIT",
  "author": "Orbs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbs-network/twap-ui.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "dependencies": {
    "@defi.org/web3-candies": "3.x",
    "ethereum-multicall": "2.x",
    "react-query": "3.x",
