    "@emotion/styled": "11.x",
    "@mui/material": "5.x",
    "react-icons": "4.x",
    "react-number-format": "^5.0.0-beta"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/react": "18.x",
    "@types/react-dom": "18.x",
    "react": "18.x",
    "react-dom": "18.x",
    "typescript": "4.x",
    "nodemon": "2.x"
  },
  "scripts": {
    "clean": "rm -rf dist && prettier -w '{src,test}/**/*.{ts,tsx,js,jsx,json,sol}'",
    "build": "npm run clean && tsc",
    "start": "nodemon --ext js,jsx,ts,tsx,json --watch ./src --exec tsc"
