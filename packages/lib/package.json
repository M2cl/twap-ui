{
  "name": "@orbs-network/twap-ui",
  "version": "0.0.1",
  "description": "TWAP UI",
  "license": "MIT",
  "author": "Orbs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbs-network/twap-ui.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "prettier": "prettier -w '{src,test}/**/*.{ts,tsx,js,jsx,json,sol}'",
    "build": "npm run prettier && rm -rf dist && tsc",
    "start": "npm run prettier && nodemon --ext js,jsx,ts,tsx,json --watch ./src --exec tsc",
    "test": "for t in eth ftm; do echo \"testing $t...\"; NETWORK=$t DOTENV_CONFIG_PATH=../../.env jest --setupFiles dotenv/config --colors; done",
    "publish": "npm publish --scope=orbs-network --access=public"
  },
  "dependencies": {
    "@defi.org/web3-candies": "4.x",
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@mui/material": "5.x",
    "@open-defi-notification-protocol/widget": "0.0.x",
    "ethereum-multicall": "2.x",
    "moment": "2.x",
    "react-ga4": "^1.4.1",
    "react-icons": "4.x",
    "react-number-format": "5.x",
    "react-query": "3.x",
    "zustand": "4.x"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.x",
    "@testing-library/react": "13.x",
    "jest": "29.x",
    "jest-environment-jsdom": "29.x",
    "ts-jest": "29.x"
  },
  "config": {
    "omit": "optional"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  }
}
